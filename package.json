{
  "name": "@girder/dicom-viewer",
  "private": false,
  "version": "0.7.0",
  "main": "dist/girder_dicom_viewer.umd.js",
  "scripts": {
    "build": "vue-cli-service build --target lib --name girder_dicom_viewer src/DicomViewer.vue",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@girder/components": "^3.0.0-alpha.4",
    "core-js": "^2.6.5",
    "daikon": "^1.2.42",
